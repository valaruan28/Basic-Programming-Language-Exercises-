// BPD0301_13.cpp : This file contains the 'main' function. Program execution begins and ends there.
/*
   Goal     : Make a Looping Program
   Program  : Displays Multiples of 5 from 100 to 135 (Ascending)
*/

#include <iostream>
using namespace std;

int main()
{
    //Mendeklarasi Variabel "bil13" dengan Tipe Data Integer
    int bil13;

    //Melakukan Perulangan "for" 
    for (bil13 = 20; bil13 < 28; bil13++)
    {
        //Melakukan Statement "if" pada Variabel "bil13"
        if (bil13 == 28)
         break;
        
        //Melakukan Perkalian 5 Setiap Bilangan dari "bil13"
        cout << bil13 * 5 << "\t";
    }
    return 0;
}

